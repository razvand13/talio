<AnchorPane prefHeight="609.0" prefWidth="926.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.OverviewOfBoardsCtrl">
